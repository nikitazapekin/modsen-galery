{
 
 "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["styled-components"],
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./components/*"],
      "@pages/*": ["./pages/*"],
